{
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["es2020", "DOM"],
    "baseUrl": "./",
    "outDir": "./compied",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "listEmittedFiles": true,
    "watch": true
  }
}
